# Alpaca Trading API 配置
# 从 https://alpaca.markets/ 获取您的API密钥
APCA_API_KEY_ID=your_api_key_here
APCA_API_SECRET_KEY=your_secret_key_here

# API基础URL
# 模拟交易 (Paper Trading): https://paper-api.alpaca.markets
# 真实交易 (Live Trading): https://api.alpaca.markets
APCA_BASE_URL=https://paper-api.alpaca.markets

# 交易配置
TRADING_SYMBOL=QQQ
TRADING_QUANTITY=1
TRIGGER_TIME=15:50

# 可选配置
# LOG_LEVEL=INFO
# CHECK_INTERVAL=60 